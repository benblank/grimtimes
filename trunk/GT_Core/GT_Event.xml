<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ../../FrameXML/UI.xsd">
	<!-- $Id -->

	<Frame name="GT_Event_TriggerFrame" enableMouse="false" hidden="false">
		<Scripts>
			<OnEvent>
				GT_Event.Trigger(event)
			</OnEvent>
			<OnLoad>
				GT_Event.OnLoad()
			</OnLoad>
			<OnUpdate>
				GT_Event.Trigger("Update")
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui>
